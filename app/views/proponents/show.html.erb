<p style="color: green"><%= notice %></p>

<h1>Proponente</h1>

<div class='pt-5'>
  <%= render @proponent %>
</div>

<div class="row pt-5">
  <div class="col-4 d-flex justify-content-between">
    <%= link_to "Atualizar", edit_proponent_path(@proponent), class: 'btn btn-primary' %>
    <%= link_to "Voltar", proponents_path, class: 'btn btn-light' %>
    <%= button_to "Destroy this proponent", @proponent, method: :delete, class: 'btn btn-danger' %>
  </div>
</div>


